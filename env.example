# 地图提供者配置
NEXT_PUBLIC_MAP_PROVIDER=mapbox

# Mapbox 配置
MAPBOX_SECRET_ACCESS_TOKEN=sk.your_mapbox_secret_token_here
MAPBOX_USERNAME=your_mapbox_username
MAPBOX_DATASET_ID=your_dataset_id_here

# 高德地图配置（未来支持）
# NEXT_PUBLIC_AMAP_ACCESS_TOKEN=your_amap_token
# NEXT_PUBLIC_AMAP_STYLE=normal

# 百度地图配置（未来支持）
# NEXT_PUBLIC_BAIDU_ACCESS_TOKEN=your_baidu_token
# NEXT_PUBLIC_BAIDU_STYLE=normal

# Google地图配置（未来支持）
# NEXT_PUBLIC_GOOGLE_ACCESS_TOKEN=your_google_token
# NEXT_PUBLIC_GOOGLE_STYLE=roadmap

# 腾讯云 COS 配置
TENCENT_COS_SECRET_ID=your_tencent_secret_id
TENCENT_COS_SECRET_KEY=your_tencent_secret_key
TENCENT_COS_REGION=ap-chongqing
TENCENT_COS_BUCKET=your_bucket_name

# 注意：NEXT_PUBLIC_ 开头的环境变量需要在 docker-compose.yml 的 args 中配置
# 这些变量会在构建时传递，不需要在 .env 文件中设置
NEXT_PUBLIC_MAPBOX_ACCESS_TOKEN=pk.your_pk_here
NEXT_PUBLIC_MAPBOX_STYLE=mapbox://styles/mapbox/streets-zh-v1
NEXT_PUBLIC_IMAGE_DOMAINS=mapannai-123456.cos.ap-chongqing.myqcloud.com